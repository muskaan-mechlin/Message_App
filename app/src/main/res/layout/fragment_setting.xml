<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".VerifiedUser.SettingFragment">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fillViewport="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagebutton"
        android:src="@drawable/ic_baseline_account_circle_24"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@color/white"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="10dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview"
        android:text="Profile name"
        android:textStyle="bold"
        android:textSize="17dp"
        android:textColor="@color/black"
        android:layout_marginTop="25dp"
        android:layout_marginRight="15dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="@id/imagebutton"
        app:layout_constraintRight_toRightOf="parent"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview1"
        android:text="About"
        android:textColor="@color/black"
        android:textSize="14dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="15dp"
        app:layout_constraintTop_toBottomOf="@id/textview"
        app:layout_constraintLeft_toLeftOf="@id/imagebutton"
        app:layout_constraintRight_toRightOf="parent"/>

    <View
        android:layout_width="wrap_content"
        android:layout_height="3dp"
        android:id="@+id/view"
        android:layout_marginTop="10dp"
        android:background="@color/browser_actions_bg_grey"
        app:layout_constraintTop_toBottomOf="@id/imagebutton"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview2"
                android:text="Accounts"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="20dp"
                app:layout_constraintTop_toBottomOf="@id/view"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview3"
                android:text="Privacy,security,change \n number"
                android:textSize="14dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/textview2"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageview0"
                android:src="@drawable/ic_baseline_vpn_key_24"
                app:layout_constraintTop_toBottomOf="@id/view"
                android:layout_marginTop="39dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="10dp"
                android:background="@color/white"/>


            <View
                android:layout_width="wrap_content"
                android:layout_height="3dp"
                android:id="@+id/view1"
                android:layout_marginTop="120dp"
                android:background="@color/browser_actions_bg_grey"
                app:layout_constraintTop_toBottomOf="@id/view"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview4"
                android:text="Chats"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="22dp"
                app:layout_constraintTop_toBottomOf="@id/view1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageview"
                android:src="@drawable/ic_baseline_message_24"
                app:layout_constraintTop_toBottomOf="@id/view1"
                android:layout_marginTop="39dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="10dp"
                android:background="@color/white"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview5"
                android:text="Theme,wallpapers,chat history"
                android:textSize="14dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/textview4"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <View
                android:layout_width="wrap_content"
                android:layout_height="3dp"
                android:id="@+id/view2"
                android:layout_marginTop="120dp"
                android:background="@color/browser_actions_bg_grey"
                app:layout_constraintTop_toBottomOf="@id/view1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>


            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageview1"
                android:src="@drawable/ic_baseline_circle_notifications_24"
                app:layout_constraintTop_toBottomOf="@id/view2"
                android:layout_marginTop="39dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="10dp"
                android:background="@color/white"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview6"
                android:text="Notifications"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="22dp"
                app:layout_constraintTop_toBottomOf="@id/view2"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview7"
                android:text="Message,group &amp; call tones"
                android:textSize="14dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/textview6"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <View
                android:layout_width="wrap_content"
                android:layout_height="3dp"
                android:id="@+id/view3"
                android:layout_marginTop="120dp"
                android:background="@color/browser_actions_bg_grey"
                app:layout_constraintTop_toBottomOf="@id/view2"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>


            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageview2"
                android:src="@drawable/ic_baseline_circle_24"
                app:layout_constraintTop_toBottomOf="@id/view3"
                android:layout_marginTop="39dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="10dp"
                android:background="@color/white"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview8"
                android:text="Storage and data"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="22dp"
                app:layout_constraintTop_toBottomOf="@id/view3"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview9"
                android:text="Network usage,auto-download"
                android:textSize="14dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/textview8"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <View
                android:layout_width="wrap_content"
                android:layout_height="3dp"
                android:id="@+id/view4"
                android:layout_marginTop="120dp"
                android:background="@color/browser_actions_bg_grey"
                app:layout_constraintTop_toBottomOf="@id/view3"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageview3"
                android:src="@drawable/ic_baseline_help_outline_24"
                app:layout_constraintTop_toBottomOf="@id/view4"
                android:layout_marginTop="39dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="10dp"
                android:background="@color/white"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview10"
                android:text="Help"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="22dp"
                app:layout_constraintTop_toBottomOf="@id/view4"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview11"
                android:text="Help centre,contact us,privacy \n policy"
                android:textSize="14dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@id/textview10"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <View
                android:layout_width="wrap_content"
                android:layout_height="3dp"
                android:id="@+id/view5"
                android:layout_marginTop="120dp"
                android:background="@color/browser_actions_bg_grey"
                app:layout_constraintTop_toBottomOf="@id/view4"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageview4"
                android:src="@drawable/ic_baseline_people_24"
                app:layout_constraintTop_toBottomOf="@id/view5"
                android:layout_marginTop="23dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="10dp"
                android:background="@color/white"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textview12"
                android:text="Invite a friend"
                android:textSize="17dp"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:layout_marginTop="22dp"
                app:layout_constraintTop_toBottomOf="@id/view5"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"/>













        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>