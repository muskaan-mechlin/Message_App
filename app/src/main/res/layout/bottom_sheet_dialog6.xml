<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagebtn"
        android:src="@drawable/icccc_baseline_insert_drive_file_24"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="2dp"
        app:layout_constraintTop_toBottomOf="@id/textview"
        app:layout_constraintLeft_toLeftOf="parent"
        android:backgroundTint="@color/white"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="20dp"
        android:text=""
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview1"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="17dp"
        android:text="Document"
        app:layout_constraintTop_toBottomOf="@id/imagebtn"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="2dp"/>


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagebtn1"
        android:src="@drawable/ic_bbbaseline_insert_photo_24"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="100dp"
        app:layout_constraintTop_toBottomOf="@id/textview"
        app:layout_constraintLeft_toLeftOf="parent"
        android:backgroundTint="@color/white"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview2"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="17dp"
        android:text="Gallery"
        app:layout_constraintTop_toBottomOf="@id/imagebtn1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="108dp"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagebtn2"
        android:src="@drawable/ic_bbbbbbaseline_photo_camera_24"
        android:layout_marginTop="17dp"
        android:layout_marginLeft="200dp"
        app:layout_constraintTop_toBottomOf="@id/textview"
        app:layout_constraintLeft_toLeftOf="parent"
        android:backgroundTint="@color/white"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview3"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="17dp"
        android:text="Camera"
        app:layout_constraintTop_toBottomOf="@id/imagebtn2"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="202dp"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagebtn3"
        android:src="@drawable/ic_baseline_headphones_24"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="2dp"
        app:layout_constraintTop_toBottomOf="@id/textview1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:backgroundTint="@color/white"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview4"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="17dp"
        android:text="Audio"
        app:layout_constraintTop_toBottomOf="@id/imagebtn3"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="14dp"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagebtn4"
        android:src="@drawable/ic_baseline_currency_rupee_24"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="100dp"
        app:layout_constraintTop_toBottomOf="@id/textview2"
        app:layout_constraintLeft_toLeftOf="parent"
        android:backgroundTint="@color/white"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview5"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="17dp"
        android:text="Payment"
        app:layout_constraintTop_toBottomOf="@id/imagebtn4"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="108dp"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagebtn5"
        android:src="@drawable/ic_baseline_location_on_24"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="200dp"
        app:layout_constraintTop_toBottomOf="@id/textview3"
        app:layout_constraintLeft_toLeftOf="parent"
        android:backgroundTint="@color/white"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview6"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="17dp"
        android:text="Location"
        app:layout_constraintTop_toBottomOf="@id/imagebtn5"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="202dp"/>


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagebtn6"
        android:src="@drawable/ic_bbbbbaseline_person_24"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="2dp"
        app:layout_constraintTop_toBottomOf="@id/textview4"
        app:layout_constraintLeft_toLeftOf="parent"
        android:backgroundTint="@color/white"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview7"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="17dp"
        android:text="Contact"
        app:layout_constraintTop_toBottomOf="@id/imagebtn6"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="7dp"/>






</androidx.constraintlayout.widget.ConstraintLayout>